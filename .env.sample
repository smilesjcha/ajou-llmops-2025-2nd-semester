# ==============================
# .env.sample
# 환경 변수 샘플 파일
# 실제 서비스 시 .env 로 복사 후 각 키를 채워주세요.
# ==============================

# 🔑 OpenAI API Key
# OpenAI API 호출 시 필요한 Secret Key
OPENAI_API_KEY="sk-~~~"

# 🔑 Langfuse Public Key
# Langfuse SDK 초기화 시 사용 (공개 가능 영역에서 쓰이는 Key)
LANGFUSE_PUBLIC_KEY="pk-lf-~~~~"

# 🔑 Langfuse Secret Key
# Langfuse SDK 초기화 시 사용 (비공개 영역에서 보관 필수)
LANGFUSE_SECRET_KEY="sk-lf-~~~~"

# 🌐 Langfuse Host
# 기본값: https://cloud.langfuse.com
LANGFUSE_HOST="https://cloud.langfuse.com"

# 🔑 Pinecone API Key
# Pinecone 벡터 데이터베이스 호출 시 필요한 Secret Key
PINECONE_API_KEY="pcsk-~~~~"

# LangSmith
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=lsv2_XXXXXXXXXXXXXXXXXXXX
# 선택: 프로젝트명 (없으면 default 프로젝트로 들어감)
LANGSMITH_PROJECT=ajou-ai-practical-project10-llmops